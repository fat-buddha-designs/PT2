---
permalink: /site.webmanifest
---
{
  "lang": "{{ site.lang }}",
  "dir": "{{ site.dir }}",
	"name": "{{ site.name }}",
	"short_name": "{{ site.name }}",
	"icons": [
		{
			"src": "/android-chrome-192x192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "/android-chrome-512x512.png",
			"sizes": "512x512",
			"type": "image/png"
		},
		{
      "src": "/maskable_icon.png",
      "sizes": "196x196",
      "type": "image/png",
      "purpose": "maskable"
    }
	],
	"theme_color": "{{ site.themeColor }}",
	"background_color": "{{ site.backgroundColor }}",
	"display": "standalone",
	"scope": "/",
	"start_url": "/"
}
