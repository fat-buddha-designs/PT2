{
  "name": "plant-theatres",
  "version": "1.0.0",
  "description": "Plant Theatres and more websites",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run eleventy:dev",
    "build": "concurrently \"npm run clean\" \"npm run eleventy:prod\" && npx pagefind --site _site ",
    "eleventy:dev": "concurrently \"npm run clean\" \"cross-env ELEVENTY_ENV=development eleventy --serve --incremental\" ",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "del-cli _site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Will Moody",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-rss": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@fat-buddha-designs/eleventy-social-images": "^0.1.1",
    "@notionhq/client": "^2.2.14",
    "@photogabble/eleventy-plugin-word-stats": "^1.0.1",
    "airtable": "^0.12.2",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "bulma": "^0.9.4",
    "common-tags": "^1.8.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del-cli": "^5.1.0",
    "dotenv": "^16.3.1",
    "eleventy-plugin-img2picture": "^5.0.0",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "eleventy-sass": "^2.2.4",
    "focus-visible": "^5.2.0",
    "html-minifier-terser": "^7.2.0",
    "humans-generator": "^2.1.1",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "lodash.replace": "^4.1.4",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-link-attributes": "^4.0.1",
    "memfs": "^4.6.0",
    "netlify-plugin-cache": "^1.0.3",
    "pagefind": "^1.0.4",
    "postcss": "^8.4.32",
    "sanitize-html": "^2.11.0",
    "webpack": "^5.89.0"
  }
}
